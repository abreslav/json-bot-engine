  "Условия": [
    { "assign": "${test_name}", "value": "else"},
    {
      "conditional": [
        {
          "condition": { "left": "x", "operation": "not_equals", "right": "x" },
          "do": [ { "goto": "Fail" } ]
        },
        { "else": [ {"goto": "OK" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "x == x"},
    {
      "conditional": [
        {
          "condition": { "left": "x", "operation": "equals", "right": "x" },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "x != y"},
    {
      "conditional": [
        {
          "condition": { "left": "x", "operation": "not_equals", "right": "y" },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "abc contains bc"},
    { "assign": "${v_bc}", "value": "bc"},
    {
      "conditional": [
        {
          "condition": { "left": "a${v_bc}", "operation": "contains", "right": "${v_bc}" },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "bc !contains abc"},
    {
      "conditional": [
        {
          "condition": { "left": "bc", "operation": "not_contains", "right": "abc" },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${v}", "value": "not_null"},
    { "assign": "${test_name}", "value": "v != null"},
    {
      "conditional": [
        {
          "condition": { "left": "${v}", "operation": "not_set"},
          "do": [ { "goto": "Fail" } ]
        },
        { "else": [ {"goto": "OK" } ] }
      ]
    },

    { "assign": "${v}", "value": null},
    { "assign": "${test_name}", "value": "v == null"},
    {
      "conditional": [
        {
          "condition": { "left": "${v}", "operation": "not_set"},
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${v}", "value": "not_null"},
    { "assign": "${test_name}", "value": "!(v == null)"},
    {
      "conditional": [
        {
          "condition": {
            "left": { "left": "${v}", "operation": "not_set"},
            "operation": "not"
          },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "(a == a) && (b == b)"},
    {
      "conditional": [
        {
          "condition": {
            "left": { "left": "a", "operation": "equals", "right": "a" },
            "operation": "and",
            "right": { "left": "b", "operation": "equals", "right": "b" }
          },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "(a == a) && (b != b)"},
    {
      "conditional": [
        {
          "condition": {
            "left": { "left": "a", "operation": "equals", "right": "a" },
            "operation": "and",
            "right": { "left": "b", "operation": "not_equals", "right": "b" }
          },
          "do": [ { "goto": "Fail" } ]
        },
        { "else": [ {"goto": "OK" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "(a != a) && (b == b)"},
    {
      "conditional": [
        {
          "condition": {
            "left": { "left": "a", "operation": "not_ equals", "right": "a" },
            "operation": "and",
            "right": { "left": "b", "operation": "equals", "right": "b" }
          },
          "do": [ { "goto": "Fail" } ]
        },
        { "else": [ {"goto": "OK" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "(a != a) && (b != b)"},
    {
      "conditional": [
        {
          "condition": {
            "left": { "left": "a", "operation": "not_ equals", "right": "a" },
            "operation": "and",
            "right": { "left": "b", "operation": "not_equals", "right": "b" }
          },
          "do": [ { "goto": "Fail" } ]
        },
        { "else": [ {"goto": "OK" } ] }
      ]
    },


    { "assign": "${test_name}", "value": "(a == a) || (b == b)"},
    {
      "conditional": [
        {
          "condition": {
            "left": { "left": "a", "operation": "equals", "right": "a" },
            "operation": "or",
            "right": { "left": "b", "operation": "equals", "right": "b" }
          },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "(a == a) || (b != b)"},
    {
      "conditional": [
        {
          "condition": {
            "left": { "left": "a", "operation": "equals", "right": "a" },
            "operation": "or",
            "right": { "left": "b", "operation": "not_equals", "right": "b" }
          },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "(a != a) || (b == b)"},
    {
      "conditional": [
        {
          "condition": {
            "left": { "left": "a", "operation": "not_ equals", "right": "a" },
            "operation": "or",
            "right": { "left": "b", "operation": "equals", "right": "b" }
          },
          "do": [ { "goto": "OK" } ]
        },
        { "else": [ {"goto": "Fail" } ] }
      ]
    },

    { "assign": "${test_name}", "value": "(a != a) || (b != b)"},
